{
  "name": "search-ehou-chrome-extension",
  "version": "2.1.0",
  "description": "Chrome extension for auto extracting and filling quiz answers using Hybrid Search (Elasticsearch + Enhanced Keyword Matching)",
  "main": "background.js",
  "scripts": {
    "build": "node build.js",
    "dev": "node build.js && echo 'Extension built. Load dist/ folder in Chrome'",
    "test": "echo 'Use Chrome DevTools Console to test CONFIG object'",
    "clean": "rm -rf dist/",
    "rebuild": "npm run clean && npm run build",
    "lint": "echo 'No linter configured yet'",
    "package": "cd dist && zip -r ../search-ehou-extension.zip ."
  },
  "keywords": [
    "chrome-extension",
    "quiz",
    "auto-fill",
    "education",
    "ehou",
    "hybrid-search",
    "elasticsearch",
    "keyword-matching"
  ],
  "author": "Search EHOU Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/search-ehou/extension"
  },
  "bugs": {
    "url": "https://github.com/search-ehou/extension/issues"
  },
  "homepage": "https://github.com/search-ehou/extension#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "extension": {
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "storage"
    ],
    "host_permissions": [
      "http://localhost:3000/*",
      "https://*.ehou.edu.vn/*"
    ]
  },
  "devDependencies": {},
  "dependencies": {},
  "config": {
    "api_base_url": "http://localhost:3000/api/v1",
    "search_threshold": 0.6,
    "elasticsearch_size": 20,
    "timeout": 30000
  }
}